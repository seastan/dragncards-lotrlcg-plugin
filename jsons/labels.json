        "rules": {
            "English": "Rules"
        },
        "step-1.1": {
            "English": "1.1: First player activates 1 battleground"
        },
        "step-1.2": {
            "English": "1.2: First player activates 1 path"
        },
        "step-2.1": {
            "English": "2.1: Players take turns taking actions or passing"
        },
        "step-3.0": {
            "English": "3.0: Starting player decides the order the battleground and path combats are resolved"
        },
        "step-3.1": {
            "English": "3.1: Count the number of attacking icons"
        },
        "step-3.2": {
            "English": "3.2: Subtract battleground or path icons"
        },
        "step-3.3": {
            "English": "3.3: Discard defending cards to subtract more icons"
        },
        "step-3.4": {
            "English": "3.4: Leftover defending cards are cycled"
        },
        "step-3.5": {
            "English": "3.5: All attacking cards are eliminated"
        },
        "step-3.6": {
            "English": "3.6: If path, Shadow team gains corruption tokens equal to excess attack"
        },
        "step-3.7": {
            "English": "3.7: Winning side takes control (Shadow team takes control of paths facedown)"
        },
        "step-4.1": {
            "English": "4.1: If the difference between scores is 10 or more, the team with the higher score wins"
        },
        "step-4.2": {
            "English": "4.2: If the last path combat has been resolved, the team with the higher score wins"
        },
        "step-5.1": {
            "English": "5.1: Draw cards"
        },
        "step-5.2": {
            "English": "5.2: Pass the starting player token to the next player"
        }
    }
