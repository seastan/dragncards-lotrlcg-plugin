                                ["TOGGLE_EXHAUST", "$PROMPT_CARD_2"],
                                ["DRAW_CARD", 5, "$CONTROLLER"],
                                ["DISCARD_CARD", "$CARD"]
                            ],
                            ["TRUE"],
                            [
                                ["LOG", "└── ", "The ability's conditions are not met."],
                                ["PROMPT", "$PLAYER_N", "peaceAndThought", "$CARD", "$CONTROLLER"]
